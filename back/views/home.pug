<!DOCTYPE html>
<html lang="zxx">
  <head>
    <title>Spartan Market | Home</title>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <!-- Favicon -->
    <!-- <link href="img/favicon.ico" rel="shortcut icon" /> -->

    <!-- Google Font -->
    <link href="https://fonts.googleapis.com/css?family=Josefin+Sans:300,300i,400,400i,700,700i" rel="stylesheet"/>
    <link href="https://fonts.googleapis.com/css?family=Caveat&display=swap" rel="stylesheet"/>

    <!-- Stylesheets -->
    <link rel="stylesheet" href="css/bootstrap.min.css"/>
    <link rel="stylesheet" href="css/font-awesome.min.css"/>
    <link rel="stylesheet" href="css/flaticon.css"/>
    <link rel="stylesheet" href="css/slicknav.min.css"/>
    <link rel="stylesheet" href="css/jquery-ui.min.css"/>
    <link rel="stylesheet" href="css/owl.carousel.min.css"/>
    <link rel="stylesheet" href="css/animate.css"/>
    <link rel="stylesheet" href="css/style.css"/>

    style.
      .spartanheader {
        max-width: 220%;
        height: auto;
      }

      .spacingnavbar {
        padding-right: 10px;
      }

      .spartanmarketfont {
        font-family: 'Caveat', cursive;
        font-size: 20px;
      }

      .description {
        font-size: 8px;
      }

    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
  </head>
  <body>
    <!-- Page Preloder -->
    <div id="preloder">
      <div class="loader"></div>
    </div>

    <!-- Header section -->
    <!-- <div class="text-center"> -->
    <header class="header-section">
      <div class="header-top">
        <div class="container">
          <div class="row">
            <div class="col-lg-2 text-center text-lg-left">
              <div class="row">
                <div class="col-lg-3">
                  <img class="spartanheader" src="img/logo.png" alt=""/>
                </div>
                <div class="col-lg-9 text-right">
                  <span class="spartanmarketfont">Spartan Market</span>
                </div>
              </div>
              <!-- logo -->
            </div>
            <div class="col-xl-6 col-lg-5">
              <form class="header-search-form">
                <input type="text" placeholder="Search for an item ...." />
                <button><i class="flaticon-search"></i></button>
              </form>
            </div>
            <div class="col-xl-4 col-lg-5">
              <div class="user-panel">
                <div class="up-item">
                  <i class="flaticon-profile"></i>
                  <a href="logout">Log Out</a>
                </div>
                <div class="up-item">
                  <div class="shopping-card">
                    <i class="flaticon-bag"></i>
                    <span>0</span>
                  </div>
                  <a href="/">Shopping Cart</a>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
      <nav class="main-navbar">
        <div class="container">
          <!-- menu -->
          <ul class="main-menu">
            <li><a href="/">Home</a></li>
            <span class="spacingnavbar"></span>
            <li><a href="#">Women</a></li>
            <span class="spacingnavbar"></span>
            <li><a href="#">Men</a></li>
            <span class="spacingnavbar"></span>
            <li><a href="#">Books</a></li>
            <span class="spacingnavbar"></span>
            <li><a href="#">Stationery</a></li>
          </ul>
        </div>
      </nav>
    </header>
    <!-- </div> -->
    <!-- Header section end -->

    <!-- latest product section -->
    <section class="top-letest-product-section">
      <div class="container">
        <div class="section-title">
          <h2>LATEST PRODUCTS</h2>
        </div>
        <div class="product-slider owl-carousel">

          - for(var i=0; i < items.length; i++) {
          <!-- 1 -->
          <div class="product-item">
            <div class="pi-pic">
              <img src="img/product/1.jpg" alt="" />
              <div class="pi-links">
                <a href="#" class="add-card"><i class="flaticon-bag"></i><span>ADD TO CART</span></a>
              </div>
            </div>
            <div class="pi-text">
              <h6>$#{items[i].Price}</h6>
              <p>#{items[i].Name}</p>
            </div>
            <p>#{items[i].Description}</p>
          </div>
          - }
        </div>
      </div>
    </section>
    <!-- letest product section end -->


    <!-- Footer section -->
    <section class="footer-section">
      <div class="container">
        <div class="row">
          <div class="col-lg-6 col-sm-12">
            <div class="footer-widget about-widget">
              <h2>About</h2>
              <p>
                | We are passinate CS students at SJSU working with the great
                | Professor Wu to accomplish this project!
              </p>
            </div>
          </div>
          <div class="col-lg-6 col-sm-12">
            <div class="footer-widget contact-widget">
              <h2>Contact Us</h2>
              <div class="con-info">
                <span>A.</span>
                <p>Maan Singh</p>
              </div>
              <div class="con-info">
                <span>B.</span>
                <p>Tien Ly</p>
              </div>
              <div class="con-info">
                <span>C.</span>
                <p>Ge Ou</p>
              </div>
            </div>
          </div>
        </div>
      </div>
      <div class="social-links-warp">
        <div class="container">
          <!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. -->
          <p class="text-white text-center mt-5">
            | Copyright &copy 
            script.
              document.write(new Date().getFullYear());

            |  All rights reserved | This template is made with 
            <i class="fa fa-heart-o" aria-hidden="true"></i> by 
            <a href="https://colorlib.com" target="_blank">Colorlib</a>
          </p>
          <p class="text-white text-center mt-5">ALL HAIL PROFESSOR WU!</p>
          <!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. -->
        </div>
      </div>
    </section>
    <!-- Footer section end -->

    <!--====== Javascripts & Jquery ======-->
    <script src="js/jquery-3.2.1.min.js"></script>
    <script src="js/bootstrap.min.js"></script>
    <script src="js/jquery.slicknav.min.js"></script>
    <script src="js/owl.carousel.min.js"></script>
    <script src="js/jquery.nicescroll.min.js"></script>
    <script src="js/jquery.zoom.min.js"></script>
    <script src="js/jquery-ui.min.js"></script>
    <script src="js/main.js"></script>
  </body>
</html>
